@article{Haapkyla:2011co,
author = {Haapkyl{\"a}, Jessica and Unsworth, Richard K F and Flavell, Mike and Bourne, David G and Schaffelke, Britta and Willis, Bette L},
title = {{Seasonal rainfall and runoff promote coral disease on an inshore reef.}},
journal = {PLoS ONE},
year = {2011},
volume = {6},
number = {2},
pages = {e16893},
affiliation = {ARC Centre of Excellence for Coral Reef Studies, School of Marine and Tropical Biology, James Cook University, Townsville, Australia. Jessica.Haapkyla@jcu.edu.au},
doi = {10.1371/journal.pone.0016893},
pmid = {21347317},
