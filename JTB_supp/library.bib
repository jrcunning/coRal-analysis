}

@article{Godinot:2011jg,
author = {Godinot, Claire and Houlbr{\`e}que, F and Grover, Renaud and Ferrier-Pag{\`e}s, Christine},
title = {{Coral Uptake of Inorganic Phosphorus and Nitrogen Negatively Affected by Simultaneous Changes in Temperature and pH}},
journal = {PLoS ONE},
year = {2011},
volume = {6},
number = {9},
pages = {e25024},
month = sep,
publisher = {Public Library of Science},
doi = {10.1371/journal.pone.0025024},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-12-09T15:05:51GMT},
date-modified = {2016-09-10T19:14:00GMT},
abstract = {The effects of ocean acidification and elevated seawater temperature on coral calcification and photosynthesis have been extensively investigated over the last two decades, whereas they are still unknown on nutrient uptake, despite their importance for coral energetics. We therefore studied the separate and combined impacts of increases in temperature and p CO 2 on phosphate, ammonium, and nitrate uptake rates by the scleractinian coral S. pistillata . Three experiments were performed, during 10 days i) at three pH T conditions (8.1, 7.8, and 7.5) and normal temperature (26{\textdegree}C), ii) at three temperature conditions (26{\textdegree}, 29{\textdegree}C, and 33{\textdegree}C) and normal pH T (8.1), and iii) at three pH T conditions (8.1, 7.8, and 7.5) and elevated temperature (33{\textdegree}C). After 10 days of incubation, corals had not bleached, as protein, chlorophyll, and zooxanthellae contents were the same in all treatments. However, photosynthetic rates significantly decreased at 33{\textdegree}C, and were further reduced for the pH T 7.5. The photosynthetic efficiency of PSII was only decreased by elevated temperature. Nutrient uptake rates were not affected by a change in pH alone. Conversely, elevated temperature (33{\textdegree}C) alone induced an increase in phosphate uptake but a severe decrease in nitrate and ammonium uptake rates, even leading to a release of nitrogen into seawater. Combination of high temperature (33{\textdegree}C) and low pH T (7.5) resulted in a significant decrease in phosphate and nitrate uptake rates compared to control corals (26{\textdegree}C, pH T  = 8.1). These results indicate that both inorganic nitrogen and phosphorus metabolism may be negatively affected by the cumulative effects of ocean warming and acidification.},
url = {http://dx.plos.org/10.1371/journal.pone.0025024},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Files/4E/4EB37783-69AA-4440-8C88-D621B30770D7.pdf},
file = {{4EB37783-69AA-4440-8C88-D621B30770D7.pdf:/Users/jrcunning/Documents/Library.papers3/Files/4E/4EB37783-69AA-4440-8C88-D621B30770D7.pdf:application/pdf;4EB37783-69AA-4440-8C88-D621B30770D7.pdf:/Users/jrcunning/Documents/Library.papers3/Files/4E/4EB37783-69AA-4440-8C88-D621B30770D7.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0025024}}
}

@article{Bethlenfalvay:1983wb,
author = {Bethlenfalvay, G J and Pacovsky, R S},
title = {{Light effects in mycorrhizal soybeans.}},
journal = {Plant Physiology},
