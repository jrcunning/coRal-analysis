@article{Edgar:2011gy,
author = {Edgar, Robert C and Haas, Brian J and Clemente, Jose C and Quince, Christopher and Knight, Rob},
title = {{UCHIME improves sensitivity and speed of chimera detection.}},
journal = {Bioinformatics (Oxford, England)},
year = {2011},
volume = {27},
number = {16},
pages = {2194--2200},
month = aug,
publisher = {Oxford University Press},
affiliation = {Tiburon, CA, USA. robert@drive5.com},
doi = {10.1093/bioinformatics/btr381},
pmid = {21700674},
pmcid = {PMC3150044},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-08-08T15:57:15GMT},
date-modified = {2016-09-10T19:14:04GMT},
abstract = {MOTIVATION:Chimeric DNA sequences often form during polymerase chain reaction amplification, especially when sequencing single regions (e.g. 16S rRNA or fungal Internal Transcribed Spacer) to assess diversity or compare populations. Undetected chimeras may be misinterpreted as novel species, causing inflated estimates of diversity and spurious inferences of differences between populations. Detection and removal of chimeras is therefore of critical importance in such experiments.

RESULTS:We describe UCHIME, a new program that detects chimeric sequences with two or more segments. UCHIME either uses a database of chimera-free sequences or detects chimeras de novo by exploiting abundance data. UCHIME has better sensitivity than ChimeraSlayer (previously the most sensitive database method), especially with short, noisy sequences. In testing on artificial bacterial communities with known composition, UCHIME de novo sensitivity is shown to be comparable to Perseus. UCHIME is >100{\texttimes} faster than Perseus and >1000{\texttimes} faster than ChimeraSlayer.

CONTACT:robert@drive5.com

AVAILABILITY:Source, binaries and data: http://drive5.com/uchime.

SUPPLEMENTARY INFORMATION:Supplementary data are available at Bioinformatics online.},
url = {http://bioinformatics.oxfordjournals.org.eres.library.manoa.hawaii.edu/content/27/16/2194.full},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2011/Edgar/Bioinformatics%202011%20Edgar.pdf},
file = {{Bioinformatics 2011 Edgar.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2011/Edgar/Bioinformatics 2011 Edgar.pdf:application/pdf;Bioinformatics 2011 Edgar.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2011/Edgar/Bioinformatics 2011 Edgar.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/bioinformatics/btr381}}
}

