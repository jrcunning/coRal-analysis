@article{Marks:2007cw,
author = {Marks, Susan and Clay, Keith},
title = {{Low resource availability differentially affects the growth of host grasses infected by fungal endophytes}},
journal = {International Journal of Plant Sciences},
year = {2007},
volume = {168},
number = {9},
pages = {1269--1277},
affiliation = {Winthrop Univ, Dept Biol, Rock Hill, SC 29733 USA},
doi = {10.1086/521834},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-02-10T21:23:10GMT},
date-modified = {2016-09-10T19:14:05GMT},
abstract = {Grasses infected with endosymbiotic ascomyceteous fungi (family Clavicipitaceae) often show increased growth and vigor; however, when resources are limited, the costs of being infected may outweigh the advantages. We grew multiple genotypes of two grasses species (Lolium perenne and Lolium arundinaceum) infected (or uninfected) with Neotyphodium sp. under limited light or water availability and measured both biomass and relative growth rates. There was a significant interaction between water stress and infection in L. perenne, while in L. arundinaceum, there were significant differences between infected and uninfected plants but no significant interaction. In both species, there were significant interactions between light level and infection. At full sun, infected plants were larger, while under reduced light, uninfected plants produced as much biomass as or more than infected plants. Although infected plants have increased vigor under high resource levels, infection will not necessarily result in increased host growth when resources are limiting. The costs and benefits of symbiotic associations should be assessed in a variety of environments to better understand the potential range of interactions.},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2&SrcAuth=mekentosj&SrcApp=Papers&DestLinkType=FullRecord&DestApp=WOS&KeyUT=000250443700004},
