@article{Bignami:2013ug,
author = {Bignami, Sean and Enochs, Ian C and Manzello, Derek P and Sponaugle, Su and Cowen, Robert K},
title = {{Ocean acidification alters the otoliths of a pantropical fish species with implications for sensory function}},
journal = {PNAS},
year = {2013},
volume = {110},
number = {18},
pages = {7366--7370},
month = apr,
publisher = {National Acad Sciences},
affiliation = {Division of Marine Biology and Fisheries, and Cooperative Institute for Marine and Atmospheric Studies, Rosenstiel School of Marine and Atmospheric Science, University of Miami, Miami, FL 33149, USA. sbignami@rsmas.miami.edu},
doi = {10.1073/pnas.1301365110},
pmid = {23589887},
pmcid = {PMC3645591},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-04-17T12:51:08GMT},
date-modified = {2015-08-02T22:01:18GMT},
abstract = {Ocean acidification affects a wide diversity of marine organisms and is of particular concern for vulnerable larval stages critical to population replenishment and connectivity. Whereas it is well known that ocean acidification will negatively affect a range of calcareous taxa, the study of fishes is more limited in both depth of understanding and diversity of study species. We used new 3D microcomputed tomography to conduct in situ analysis of the impact of ocean acidification on otolith (ear stone) size and density of larval cobia (Rachycentron canadum), a large, economically important, pantropical fish species that shares many life history traits with a diversity of high-value, tropical pelagic fishes. We show that 2,100 $\mu$atm partial pressure of carbon dioxide (pCO2) significantly increased not only otolith size (up to 49% greater volume and 58% greater relative mass) but also otolith density (6% higher). Estimated relative mass in 800 $\mu$atm pCO2 treatments was 14% greater, and there was a similar but nonsignificant trend for otolith size. Using a modeling approach, we demonstrate that these changes could affect auditory sensitivity including a ∼50% increase in hearing range at 2,100 $\mu$atm pCO2, which may alter the perception of auditory information by larval cobia in a high-CO2 ocean. Our results indicate that ocean acidification has a graded effect on cobia otoliths, with the potential to substantially influence the dispersal, survival, and recruitment of a pelagic fish species. These results have important implications for population maintenance/replenishment, connectivity, and conservation efforts for other valuable fish stocks that are already being deleteriously impacted by overfishing.},
url = {http://www.pnas.org/content/110/18/7366.full},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2013/Bignami/PNAS%202013%20Bignami.pdf},
file = {{PNAS 2013 Bignami.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/Bignami/PNAS 2013 Bignami.pdf:application/pdf;PNAS 2013 Bignami.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/Bignami/PNAS 2013 Bignami.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1301365110}}
}

