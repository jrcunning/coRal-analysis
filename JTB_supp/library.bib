@article{ZapienCampos:2015dz,
author = {Zapi{\'e}n-Campos, Rom{\'a}n and Olmedo-{\'A}lvarez, Gabriela and Santill{\'a}n, Mois{\'e}s},
title = {{Antagonistic interactions are sufficient to explain self-assemblage of bacterial communities in a homogeneous environment: a computational modeling approach}},
journal = {Frontiers in Microbiology},
year = {2015},
volume = {6},
month = may,
publisher = {Frontiers},
doi = {10.3389/fmicb.2015.00489},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-08T04:52:26GMT},
date-modified = {2016-09-10T19:14:20GMT},
abstract = {Most of the studies in Ecology have been devoted to analyzing the effects the environment has on individuals, populations, and communities, thus neglecting the effects of biotic interactions on the system dynamics. In the present work we study the structure of bacterial communities in the oligotrophic shallow water system of Churince, Cuatro Cienegas, Mexico. Since the physicochemical conditions of this water system are homogeneous and quite stable in time, it is an excellent candidate to study how biotic factors influence the structure of bacterial communities. In a previous study, the binary antagonistic interactions of 78 bacterial strains, isolated from Churince, were experimentally determined. We employ these data to develop a computer algorithm to simulate growth experiments in a cellular grid representing the pond. Remarkably, in our model, the dynamics of all the simulated bacterial populations is determined solely by antagonistic interactions. Our results indicate that all bacterial strains (even those that are antagonized by many other bacteria) survive in the long term, and that the underlying mechanism is the formation of bacterial community patches. Patches corresponding to less antagonistic and highly susceptible strains are consistently isolated from the highly-antagonistic bacterial colonies by patches of neutral strains. These results concur with the observed features of the bacterial community structure previously reported. Finally, we study how our findings depend on factors like initial population size, differential population growth rates, homogeneous population death rates, and enhanced bacterial diffusion.},
url = {http://journal.frontiersin.org/article/10.3389/fmicb.2015.00489/full},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2015/Zapi%C3%A9n-Campos/Frontiers%20in%20Microbiology%202015%20Zapi%C3%A9n-Campos.pdf},
file = {{Frontiers in Microbiology 2015 Zapién-Campos.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2015/Zapién-Campos/Frontiers in Microbiology 2015 Zapién-Campos.pdf:application/pdf;Frontiers in Microbiology 2015 Zapién-Campos.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2015/Zapién-Campos/Frontiers in Microbiology 2015 Zapién-Campos.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.3389/fmicb.2015.00489}}
}

