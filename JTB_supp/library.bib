@article{Orrenius:2007p4079,
author = {Orrenius, Sten and Gogvadze, Vladimir and Zhivotovsky, Boris},
title = {{Mitochondrial oxidative stress: Implications for cell death}},
journal = {Annual Review Of Pharmacology And Toxicology},
year = {2007},
volume = {47},
pages = {143--183},
affiliation = {Karolinska Inst, Inst Environm Med, S-17177 Stockholm, Sweden},
doi = {10.1146/annurev.pharmtox.47.120505.105122},
language = {English},
rating = {0},
date-added = {2009-02-04T18:20:40GMT},
date-modified = {2015-04-19T14:33:55GMT},
abstract = {In addition to the established role of the mitochondria in energy 1 metabolism, regulation of cell death has emerged as a second major function of these organelles. This seems to be intimately linked to their generation of reactive oxygen species (ROS), which have been implicated in mtDNA mutations, aging, and cell death. Mitochondrial regulation of apoptosis occurs by mechanisms, which have been conserved through evolution. Thus, many lethal agents target the mitochondria and cause release of cytochrome c and other pro-apoptotic proteins into the cytoplasm. Cytochrome c release is initiated by the dissociation of the hemoprotein from its binding to the inner mitochondrial membrane. Oxidation of cardiolipin reduces cytochrome c binding and increases the level of soluble cytochrome c in the intermembrane space. Subsequent release of the hemoprotein occurs by pore formation mediated by pro-apoptotic Bcl-2 family proteins, or by Ca2+ and ROS-triggered mitochondrial permeability transition, although the latter pathway might be more closely associated with necrosis. Taken together, these findings have placed the mitochondria in the focus of current cell death research.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2007/Orrenius/Annu%20Rev%20Pharmacol%202007%20Orrenius.pdf},
file = {{Annu Rev Pharmacol 2007 Orrenius.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2007/Orrenius/Annu Rev Pharmacol 2007 Orrenius.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1146/annurev.pharmtox.47.120505.105122}}
}

