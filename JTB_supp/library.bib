journal = {Annual Review of Genetics},
year = {2004},
volume = {38},
number = {1},
pages = {119--173},
affiliation = {The Carnegie Institution of Washington, Department of Plant Biology, Stanford, California 94305, USA. arthurg@stanford.edu},
