file = {{Mar Poll Bull 2002 Owen.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2002/Owen/Mar Poll Bull 2002 Owen.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/0463D830-0894-4B9B-8992-4608EE4A740C}}
}

