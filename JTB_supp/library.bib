@article{McDaniel:2010ks,
author = {McDaniel, Lauren D and Young, Elizabeth and Delaney, Jennifer and Ruhnau, Fabian and Ritchie, Kim B and Paul, John H},
title = {{High frequency of horizontal gene transfer in the oceans.}},
journal = {Science},
year = {2010},
volume = {330},
number = {6000},
pages = {50--50},
month = oct,
affiliation = {University of South Florida College of Marine Science, St. Petersburg, FL 33701, USA. mcdaniel@marine.usf.edu},
doi = {10.1126/science.1192243},
pmid = {20929803},
language = {English},
rating = {0},
date-added = {2013-02-21T16:58:26GMT},
date-modified = {2016-09-10T19:13:52GMT},
abstract = {Oceanic bacteria perform many environmental functions, including biogeochemical cycling of many elements, metabolizing of greenhouse gases, functioning in oceanic food webs (microbial loop), and producing valuable natural products and viruses. We demonstrate that the widespread capability of marine bacteria to participate in horizontal gene transfer (HGT) in coastal and oceanic environments may be the result of gene transfer agents (GTAs), viral-like particles produced by $\alpha$-Proteobacteria. We documented GTA-mediated gene transfer frequencies a thousand to a hundred million times higher than prior estimates of HGT in the oceans, with as high as 47% of the culturable natural microbial community confirmed as gene recipients. These findings suggest a plausible mechanism by which marine bacteria acquire novel traits, thus ensuring resilience in the face of environmental change.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1192243},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2010/McDaniel/Science%202010%20McDaniel.pdf},
