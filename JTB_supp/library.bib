@article{Knowlton:2001p3507,
author = {Knowlton, Nancy},
title = {{The future of coral reefs}},
journal = {Proceedings Of The National Academy Of Sciences Of The United States Of America},
year = {2001},
volume = {98},
number = {10},
pages = {5419--5425},
affiliation = {Univ Calif San Diego, Scripps Inst Oceanog, Marine Biol Res Div 0202, La Jolla, CA 92093 USA},
language = {English},
rating = {0},
date-added = {2009-02-04T18:21:09GMT},
date-modified = {2015-04-19T14:37:48GMT},
abstract = {Coral reefs, with their millions of species, have changed profoundly because of the effects of people, and will continue to do so for the foreseeable future. Reefs are subject to many of the same processes that affect other human-dominated ecosystems, but some special features merit emphasis: (i) Many dominant reef builders spawn eggs and sperm into the water column, where fertilization occurs. They are thus particularly vulnerable to Allee effects, including potential extinction associated with chronic reproductive failure. (ii) The corals likely to be most resistant to the effects of habitat degradation are small, short-lived "weedy" corals that have limited dispersal capabilities at the larval stage. Habitat degradation, together with habitat fragmentation, will therefore lead to the establishment of genetically isolated clusters of inbreeding corals. (iii) Increases in average sea temperatures by as little as 1 degreesC, a likely result of global climate change, can cause coral "bleaching" (the breakdown of coral-algal symbiosis), changes in symbiont communities, and coral death. (iv) The activities of people near reefs increase both fishing pressure and nutrient inputs. In general, these processes favor more rapidly growing competitors, often fleshy seaweeds, and may also result in explosions of predator populations. (v) Combinations of stress appear to be associated with threshold responses and Ecological surprises, including devastating pathogen outbreaks. (vi) The fossil record suggests that corals as a group are more likely to suffer extinctions than some of the groups that associate with them, whose habitat requirements may be less stringent.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2001/Knowlton/Proc%20Natl%20Acad%20Sci%20USA%202001%20Knowlton.pdf},
file = {{Proc Natl Acad Sci USA 2001 Knowlton.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2001/Knowlton/Proc Natl Acad Sci USA 2001 Knowlton.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/7D3A32B8-840E-4424-9E33-968948435E76}}
}

