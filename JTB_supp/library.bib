@article{Lingwood:2005p4304,
author = {Lingwood, D and Harauz, G and Ballantyne, JS},
title = {{Regulation of fish gill Na+-K+-ATPase by selective sulfatide-enriched raft partitioning during seawater adaptation}},
journal = {Journal of Biological Chemistry},
year = {2005},
volume = {280},
number = {44},
pages = {36545--36550},
affiliation = {Univ Guelph, Dept Integrat Biol, Guelph, ON N1G 2W1, Canada},
doi = {10.1074/jbc.M506670200},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-03-02T15:37:09GMT},
date-modified = {2016-09-10T19:16:49GMT},
abstract = {Na+- K+-ATPase is arguably the most important enzyme in the animal cell plasma membrane, but the role of the membrane in its regulation is poorly understood. We investigated the relationship between Na+-K+-ATPase and membrane microdomains or "lipid rafts" enriched in sulfatide ( sulfogalactosylceramide/SGC), a glycosphingolipid implicated as a cofactor for this enzyme, in the basolateral membrane of rainbow trout gill epithelium. Our studies demonstrated that when trout adapt to seawater (33 ppt), Na+- K+-ATPase relocates to these structures. Arylsulfatase-induced desulfation of basolateral membrane SGC prevented this relocation and significantly reduced Na+- K+-ATPase activity in seawater but not freshwater trout. We contend that Na+- K+-ATPase partitions into SGC-enriched rafts to help facilitate the up-regulation of its activity during seawater adaptation. We also suggest that differential partitioning of Na+- K+-ATPase between these novel SGC-enriched regulatory platforms results in two distinct, physiological Na+ transport modes. In addition, we extend the working definition of cholesterol-dependent raft integrity to structural dependence on the sulfate moiety of SGC in this membrane.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2005/Lingwood/Journal%20of%20Biological%20Chemistry%202005%20Lingwood.pdf},
file = {{Journal of Biological Chemistry 2005 Lingwood.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2005/Lingwood/Journal of Biological Chemistry 2005 Lingwood.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1074/jbc.M506670200}}
}

