@article{Shafir:2007p6228,
author = {Shafir, Shai and Van Rijn, Jaap and Rinkevich, Baruch},
title = {{Short and long term toxicity of crude oil and oil dispersants to two representative coral species}},
journal = {Environmental Science {\&} Technology},
year = {2007},
volume = {41},
number = {15},
pages = {5571--5574},
annote = {pollutants - oil
},
affiliation = {Natl Inst Oceanog, Israel Oceanog {\&} Limnol Res, IL-31080 Haifa, Israel},
doi = {10.1021/es0704582},
language = {English},
rating = {0},
date-added = {2009-04-08T22:20:09GMT},
date-modified = {2015-04-19T14:33:00GMT},
abstract = {Oil dispersants, the tool of choice for treating oil spills in tropical marine environments, is potentially harmful to marine life, including reef corals. In a previous study, we found that dispersed oil and oil dispersants are harmful to soft and hard coral species at early life stages. In this broader study, we employed a "nubbin assay" on more than 10 000 coral fragments to evaluate the short- and long-term impacts of dispersed oil fractions (DOFs) from six commercial dispersants, the dispersants and water-soluble-fractions (WSFs) of Egyptian crude oil, on two Indo Pacific branching coral species, Stylophora pistillata and Pocillopora damicornis. Survivorship and growth of nubbins were recorded for up to 50 days following a single, short (24 h) exposure to toxicants in various concentrations. Manufacturer-recommended dispersant concentrations proved to be highly toxic and resulted in mortality for all nubbins. The dispersed oil and the dispersants were significantly more toxic than crude oil WSFs. As corals are particularly susceptible to oil detergents and dispersed oil, the results of these assays rules out the use of any oil dispersant in coral reefs and in their vicinity. The ecotoxicological impacts of the various dispersants on the corals could be rated on a scale from the least to the most harmful agent, as follows: Slickgone > Petrotech > Inipol = Biorieco > Emulgal > Dispolen.},
url = {http://pubs.acs.org/doi/abs/10.1021/es0704582},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2007/Shafir/Environ.%20Sci.%20Technol.%202007%20Shafir.pdf},
file = {{Environ. Sci. Technol. 2007 Shafir.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2007/Shafir/Environ. Sci. Technol. 2007 Shafir.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/es0704582}}
}

