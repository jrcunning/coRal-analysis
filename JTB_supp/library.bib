@article{Dixon:2014gv,
author = {Dixon, Groves B and Bay, Line K and Matz, Mikhail V},
title = {{Bimodal signatures of germline methylation are linked with gene expression plasticity in the coral Acropora millepora.}},
journal = {BMC Genomics},
year = {2014},
volume = {15},
number = {1},
pages = {1109},
publisher = {BioMed Central Ltd},
affiliation = {Institute for Cell and Molecular Biology, University of Texas Austin, Austin, USA. grovesdixon@utexas.edu.},
doi = {10.1186/1471-2164-15-1109},
pmid = {25511458},
pmcid = {PMC4378018},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-11-01T17:53:49GMT},
date-modified = {2016-09-10T19:14:07GMT},
abstract = {BACKGROUND:In invertebrates, genes belonging to dynamically regulated functional categories appear to be less methylated than "housekeeping" genes, suggesting that DNA methylation may modulate gene expression plasticity. To date, however, experimental evidence to support this hypothesis across different natural habitats has been lacking.

RESULTS:Gene expression profiles were generated from 30 pairs of genetically identical fragments of coral Acropora millepora reciprocally transplanted between distinct natural habitats for 3 months. Gene expression was analyzed in the context of normalized CpG content, a well-established signature of historical germline DNA methylation. Genes with weak methylation signatures were more likely to demonstrate differential expression based on both transplant environment and population of origin than genes with strong methylation signatures. Moreover, the magnitude of expression differences due to environment and population were greater for genes with weak methylation signatures.

CONCLUSIONS:Our results support a connection between differential germline methylation and gene expression flexibility across environments and populations. Studies of phylogenetically basal invertebrates such as corals will further elucidate the fundamental functional aspects of gene body methylation in Metazoa.},
url = {http://www.biomedcentral.com/1471-2164/15/1109},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Files/0D/0D0D72E8-62DF-41A6-9B55-BF66A35AFA8B.pdf},
file = {{0D0D72E8-62DF-41A6-9B55-BF66A35AFA8B.pdf:/Users/jrcunning/Documents/Library.papers3/Files/0D/0D0D72E8-62DF-41A6-9B55-BF66A35AFA8B.pdf:application/pdf;0D0D72E8-62DF-41A6-9B55-BF66A35AFA8B.pdf:/Users/jrcunning/Documents/Library.papers3/Files/0D/0D0D72E8-62DF-41A6-9B55-BF66A35AFA8B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/1471-2164-15-1109}}
}

