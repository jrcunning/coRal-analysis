@article{Tchernov:2011dw,
author = {Tchernov, Dan and Kvitt, Hagit and Haramaty, Liti and Bibby, Thomas S and Gorbunov, Maxim Y and Rosenfeld, Hanna and Falkowski, Paul G},
title = {{Apoptosis and the selective survival of host animals following thermal bleaching in zooxanthellate corals.}},
journal = {Proceedings Of The National Academy Of Sciences Of The United States Of America},
year = {2011},
volume = {108},
number = {24},
pages = {9905--9909},
month = jun,
affiliation = {Marine Biology Department, Leon H Charney School of Marine Sciences, University of Haifa, Haifa 31905, Israel. dtchernov@gmail.com},
doi = {10.1073/pnas.1106924108},
pmid = {21636790},
pmcid = {PMC3116386},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-08-10T16:54:02GMT},
date-modified = {2016-09-10T19:13:56GMT},
abstract = {During the past several decades, numerous reports from disparate geographical areas have documented an increased frequency of "bleaching" in reef-forming corals. The phenomenon, triggered by increased sea surface temperatures, occurs when the cnidarian hosts digest and/or expel their intracellular, photosynthetic dinoflagellate symbionts ("zooxanthellae" in the genus Symbiodinium). Although coral bleaching is often followed by the death of the animal hosts, in some cases, the animal survives and can be repopulated with viable zooxanthellae. The physiological factors determining the ability of the coral to survive bleaching events are poorly understood. In this study, we experimentally established that bleaching and death of the host animal involve a caspase-mediated apoptotic cascade induced by reactive oxygen species produced primarily by the algal symbionts. In addition, we demonstrate that, although some corals naturally suppress caspase activity and significantly reduce caspase concentration under high temperatures as a mechanism to prevent colony death from apoptosis, even sensitive corals can be prevented from dying by application of exogenous inhibitors of caspases. Our results indicate that variability in response to thermal stress in corals is determined by a four-element, combinatorial genetic matrix intrinsic to the specific symbiotic association. Based on our experimental data, we present a working model in which the phenotypic expression of this symbiont/host relationship places a selective pressure on the symbiotic association. The model predicts the survival of the host animals in which the caspase-mediated apoptotic cascade is down-regulated.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21636790&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2011/Tchernov/Proc%20Natl%20Acad%20Sci%20USA%202011%20Tchernov.pdf},
file = {{Proc Natl Acad Sci USA 2011 Tchernov.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2011/Tchernov/Proc Natl Acad Sci USA 2011 Tchernov.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1106924108}}
}

