@article{ShoreMaggio:2015di,
author = {Shore-Maggio, Amanda and Runyon, Christina M and Ushijima, Blake and Aeby, Greta S and Callahan, Sean M},
title = {{Differences in bacterial community structure in two color morphs of the Hawaiian reef coral, Montipora capitata}},
journal = {Applied and Environmental Microbiology},
year = {2015},
pages = {AEM.01935--15},
month = aug,
publisher = {American Society for Microbiology},
affiliation = {Department of Microbiology, University of Hawaii at Manoa Honolulu, HI, USA Hawaii Institute of Marine Biology, SOEST, University of Hawaii, Honolulu, HI, USA.},
doi = {10.1128/AEM.01935-15},
pmid = {26253663},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-11T02:40:31GMT},
date-modified = {2016-09-10T19:14:22GMT},
abstract = {Corals harbor diverse bacterial associations that contribute to the health of the host. Using 16S rRNA pyrosequencing, we compared the bacterial communities of red and orange color morphs of the Hawaiian coral, Montipora capitata. Although both color morphs shared dominant bacterial genera, weighted and unweighted UniFrac analysis showed distinct bacterial communities. A single OTU, classified as Vibrio, represented the biggest driver of differences between the color morphs. This OTU was 35.4% ({\textpm} 5.5) of the orange morph bacterial community, yet was 1.1% ({\textpm} 0.6) of the red morph bacterial community. Cultivable bacteria from the two color morphs were also compared and tested for antibacterial activity. Cultured isolates represented 14 genera (7% of total genera identified from sequencing data) and all but two cultured isolates had a matching OTU from sequencing data. Half of the isolates tested (8 out of 16) displayed antibacterial activity against other cultured isolates but not against two known bacterial pathogens of M. capitata. The results from this study demonstrate that the specificity of coral-bacterial associations extends beyond the level of coral species. In addition, culture-dependent methods captured bacterial diversity that was representative of both rare and abundant members of the associated bacterial community, as characterized by culture independent methods.},
url = {http://aem.asm.org/lookup/doi/10.1128/AEM.01935-15},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2015/Shore-Maggio/Appl%20Environ%20Microbiol%202015%20Shore-Maggio.pdf},
file = {{Appl Environ Microbiol 2015 Shore-Maggio.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2015/Shore-Maggio/Appl Environ Microbiol 2015 Shore-Maggio.pdf:application/pdf;Appl Environ Microbiol 2015 Shore-Maggio.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2015/Shore-Maggio/Appl Environ Microbiol 2015 Shore-Maggio.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1128/AEM.01935-15}}
}

