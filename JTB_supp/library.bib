@article{Wangpraseurt:2014jn,
author = {Wangpraseurt, Daniel and Tamburic, Bojan and Szab{\'o}, Mil{\'a}n and Suggett, David and Ralph, Peter J and K{\"u}hl, Michael},
title = {{Spectral Effects on Symbiodinium Photobiology Studied with a Programmable Light Engine}},
journal = {PLoS ONE},
year = {2014},
volume = {9},
number = {11},
pages = {e112809},
month = nov,
publisher = {Public Library of Science},
doi = {10.1371/journal.pone.0112809},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-12-14T21:42:26GMT},
date-modified = {2016-09-10T19:13:57GMT},
abstract = {The spectral light field of Symbiodinium within the tissue of the coral animal host can deviate strongly from the ambient light field on a coral reef and that of artificial light sources used in lab studies on coral photobiology. Here, we used a novel approach involving light microsensor measurements and a programmable light engine to reconstruct the spectral light field that Symbiodinium is exposed to inside the coral host and the light field of a conventional halogen lamp in a comparative study of Symbiodinium photobiology. We found that extracellular gross photosynthetic O 2 evolution was unchanged under different spectral illumination, while the more red-weighted halogen lamp spectrum decreased PSII electron transport rates and there was a trend towards increased light-enhanced dark respiration rates under excess irradiance. The approach provided here allows for reconstructing and comparing intra-tissue coral light fields and other complex spectral compositions of incident irradiance. This novel combination of sensor technologies provides a framework to studying the influence of macro- and microscale optics on Symbiodinium photobiology with unprecedented spectral resolution.},
url = {http://dx.plos.org/10.1371/journal.pone.0112809},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Files/D3/D3C7BD8A-4484-4AE5-BD8D-525FFDD1E46E.pdf},
file = {{D3C7BD8A-4484-4AE5-BD8D-525FFDD1E46E.pdf:/Users/jrcunning/Documents/Library.papers3/Files/D3/D3C7BD8A-4484-4AE5-BD8D-525FFDD1E46E.pdf:application/pdf;D3C7BD8A-4484-4AE5-BD8D-525FFDD1E46E.pdf:/Users/jrcunning/Documents/Library.papers3/Files/D3/D3C7BD8A-4484-4AE5-BD8D-525FFDD1E46E.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0112809}}
}

