@article{Steen:1987p1326,
author = {Steen, RG and Muscatine, Leonard},
title = {{Low Temperature Evokes Rapid Exocytosis of Symbiotic Algae by a Sea Anemone}},
journal = {The Biological Bulletin},
year = {1987},
volume = {172},
pages = {246--263},
month = apr,
annote = {low temperature},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2008-11-20T18:42:34GMT},
date-modified = {2016-09-10T19:16:57GMT},
abstract = {Darkness evokes expulsion of zooxanthellae from the sea anemone Aiptasia pulchella, but brief exposure to low temperature (4{\textdegree}C, 4 h) increases the expulsion rate four-fold. Ninety-eight percent of the zooxanthellae are expelled within four days. Low temperature incubation had no detectable effect on host animal survival or behavior, but the effect on zooxanthellae was profound. Low temperature reduced the rate of photosynthesis, increased the rate of release of fixed carbon, reduced the number of viable cells and decreased the rate of cell division. Electron micrographs revealed that low temperature elicited the appearance of a thickened cell envelope and novel electron-dense inclusions, tentatively identified as crystallized lipoprotein. Immediately after low temperature incubation zooxanthellae move towards the apex of the host cells and are released to the coelenteron by exocytosis. The mechanisms by which low temperature may evoke exocytosis of zooxanthellae (e.g., disassembly of host cell microtubules; increased concentration of cytosolic calcium ions) are discussed.},
url = {http://www.jstor.org/stable/1541797},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/1987/Steen/Biol%20Bull%201987%20Steen.pdf},
