@article{Crawford:1999p4291,
author = {Crawford, Douglas and Segal, JA and Barnett, JL},
title = {{Evolutionary analysis of TATA-less proximal promoter function}},
journal = {Molecular Biology And Evolution},
year = {1999},
volume = {16},
number = {2},
pages = {194--207},
affiliation = {Univ Missouri, Div Mol Biol {\&} Biochem, Kansas City, MO 64110 USA},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-03-02T15:36:20GMT},
date-modified = {2016-09-10T19:17:07GMT},
abstract = {Many molecular studies describe how components of the proximal promoter affect transcriptional processes. However, these studies do not account for the likely effects of distant enhancers or chromatin structure, and thus it is difficult to conclude that the sequence variation in proximal promoters acts to modulate transcription in the natural context of the whole genome. This problem, the biological importance of proximal promoter sequence variation, can be addressed using a combination of molecular and evolutionary analyses. Provided here are molecular and evolutionary analyses of the variation in promoter function and sequence within and between populations of Fundulus heteroclitus for the lactate dehydrogenase-B (Ldh-B) proximal promoter. Approximately one third of the Ldh-B proximal promoter contains interspersed regions that are functionally important: (1) they bind transcription factors in vivo, (2) they effect a change in transcription as assayed by transient transfection into two different fish cell lines, and (3) they bind purified transcription factors in vitro. Evolutionary analyses that compare sequence variation in these functional regions versus the nonfunctional regions indicate that the changes in the Ldh-B proximal promoter sequences are due to directional selection. Thus, the Ldh-B proximal promoter sequence variations that affect transcriptional processes constitute a phenotypic change that is subject to natural selection, suggesting that proximal promoter sequence variation affects transcription in the natural context of the whole genome.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/1999/Crawford/Mol%20Biol%20Evol%201999%20Crawford.pdf},
file = {{Mol Biol Evol 1999 Crawford.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/1999/Crawford/Mol Biol Evol 1999 Crawford.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/365B3235-4435-49E3-8B5D-197C9272EF90}}
}

