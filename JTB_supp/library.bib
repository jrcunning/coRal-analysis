@article{Salih:2000p3809,
author = {Salih, Anya and Larkum, Anthony W D and Cox, Guy and K{\"u}hl, Michael and Hoegh-Guldberg, Ove},
title = {{Fluorescent pigments in corals are photoprotective}},
journal = {Nature},
year = {2000},
volume = {408},
number = {6814},
pages = {850--853},
affiliation = {Univ Sydney, Sch Biol Sci, Sydney, NSW 2006, Australia},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-02-04T18:19:07GMT},
date-modified = {2016-09-10T19:17:03GMT},
abstract = {All reef-forming corals depend on the photosynthesis performed by their algal symbiont, and such corals are therefore restricted to the photic zone. The intensity of light in this zone declines over several orders of magnitude-from high and damaging levels at the surface to extreme shade conditions at the lower limit(1). The ability of corals to tolerate this range implies effective mechanisms for light acclimation and adaptation(2). Here we show that the fluorescent pigments(3-9) (FPs) of corals provide a photobiological system for regulating the light environment of coral host tissue. Previous studies have suggested that under low light, FPs may enhance light availability(4,5). We now report that in excessive sunlight FPs are photoprotective; they achieve this by dissipating excess energy at wavelengths of low photosynthetic activity, as well as by reflecting of visible and infrared light by FP-containing chromatophores. We also show that FPs enhance the resistance to mass bleaching of corals during periods of heat stress, which has implications for the effect of environmental stress on the diversity of reef-building corals, such as enhanced survival of a broad range of corals allowing maintenance of habitat diversity.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2000/Salih/Nature%202000%20Salih.pdf},
file = {{Nature 2000 Salih.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2000/Salih/Nature 2000 Salih.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/EC60F55D-4BFB-48E1-B54D-657B9A5B8241}}
}

