file = {{Annu. Rev. Microbiol. 1996 Ruby.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/1996/Ruby/Annu. Rev. Microbiol. 1996 Ruby.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1146/annurev.micro.50.1.591}}
}

