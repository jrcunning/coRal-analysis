journal = {Annual review of genomics and human genetics},
year = {2008},
volume = {9},
number = {1},
pages = {387--402},
affiliation = {Department of Genetics and Molecular Microbiology and Genome Sequencing Center, Washington University School of Medicine, St. Louis MO 63108, USA. emardis@wustl.edu},
