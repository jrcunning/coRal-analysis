@article{Okamoto:2005p3736,
author = {Okamoto, M and Nojima, Satoshi and Furushima, Y and Nojima, H},
title = {{Evaluation of coral bleaching condition in situ using an underwater pulse amplitude modulated fluorometer}},
journal = {Fisheries Science},
year = {2005},
volume = {71},
number = {4},
pages = {847--854},
affiliation = {Tokyo Univ Marine Sci {\&} Technol, Tokyo 1088477, Japan},
language = {English},
rating = {0},
date-added = {2009-02-04T18:20:40GMT},
date-modified = {2015-04-19T14:36:05GMT},
abstract = {Photosynthetic activities of hermatypic corals (28 genera, c. 68 species and c. 92 specimen) were measured in November 2000 at Sekisei lagoon, Okinawa, Japan, using an underwater pulse amplitude modulated fluorometer. Maximum quantum yield (Fv/Fm) ranged from 0.61 to 0.70 and had an average value of 0.664 (standard deviation [SD], 0.0289). Following the measurement of Fv/Fm, quantum yield (Fm'-F)/Fm' at eight-leveled actinic-light intensities were obtained from all specimens. Values of electronic transport rates (ETR) were nearly the same under low photosynthetic active radiation (PAR; mu mol quanta/m(2) per s) condition and varied with increasing PAR. The value of ETR at 100 pmol quanta/m(2) per s ranged from 21.3 to 30.0 and had an average of 27.8 (SD, 2.09). In August 2001, a coral bleaching event was observed in Sekisei Lagoon. During the event, photosynthetic activities of 21 specimens of eight species (seven genera) were measured. By applying two indicators, Fv/Fm and ETR at 100 pmol quanta/m2 per s obtained in August 2000, 18 of 21 specimens were suggested under the bleaching process. In case of Porites lutea, seven colonies were visually evaluated for their condition of bleaching and were found to have two `normal', three `slightly bleached' and two `heavily bleached'. From the density of zooxanthellae, colonies evaluated `normal' or 'slightly bleached' did not show an apparent difference. However, they were clearly identified by Fm/Fv and ETR at 100 mu mol quanta/m(2) per s.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2005/Okamoto/Fisheries%20Science%202005%20Okamoto.pdf},
file = {{Fisheries Science 2005 Okamoto.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2005/Okamoto/Fisheries Science 2005 Okamoto.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/2F4149F5-1F31-45A1-852F-741510ED8F5C}}
}

