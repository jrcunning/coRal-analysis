file = {{J Exp Mar Biol Ecol 1995 Riegl.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/1995/Riegl/J Exp Mar Biol Ecol 1995 Riegl.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/9F4B60D4-A135-449A-8EE9-80D435F0F4BD}}
}

