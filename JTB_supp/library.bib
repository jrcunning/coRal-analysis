@article{Titlyanov:1998p3893,
author = {Titlyanov, E A and Titlyanova, T V and Loya, Yossi and Yamazato, K},
title = {{Degradation and proliferation of zooxanthellae in planulae of the hermatypic coral Stylophora pistillata}},
journal = {Marine Biology},
year = {1998},
