file = {{Mar Poll Bull 2009 Obura.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2009/Obura/Mar Poll Bull 2009 Obura.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.marpolbul.2008.10.002}}
}

