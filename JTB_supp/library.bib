@article{Kerswell:2003p326,
author = {Kerswell, Ailsa P and Jones, Ross J},
title = {{Effects of hypo-osmosis on the coral Stylophora pistillata: nature and cause of 'low-salinity bleaching'}},
journal = {Marine Ecology Progress Series},
year = {2003},
volume = {253},
pages = {145--154},
month = may,
annote = {salinity},
read = {Yes},
rating = {0},
date-added = {2008-11-10T20:37:39GMT},
date-modified = {2016-09-10T19:16:43GMT},
abstract = {The response of the scleractinian coral Stylophora pistillatato short-term (min to h) and long-term (d) exposure to reduced-salinity seawater was examined. Pulse Amplitude Modulated (PAM) chlorophyll fluorescence techniques were used to assess the photosynthetic efficiency of the symbiotic dinoflagellates (dark-adapted Fv/Fm) in the coral tissues (in hospite) before, during and after exposure. Exposure to reduced-salinity seawater caused a marked reduction in efficiency (the ratio of variable [Fv] to maximal [Fm] fluorescence), and there was an apparent link between a reduction in dark-adapted Fv/Fmand a loss of symbiotic dinoflagellates from the corals. The reduction in Fv/Fmof the symbiotic algae and subsequent dissociation of the coral-algal symbiosis (coral bleaching) occurred during exposure to reduced-salinity seawater in either the light or dark. The results demonstrate that bleaching in response to low-salinity seawater is a truly sublethal response, contrary to a recent suggestion. The study also suggests that bleaching of corals in response to low-salinity seawater may not involve the passive loss of algal symbionts and that an impairment of the capacity of the algal symbionts for photosynthesis represents a common {\textquoteleft}cue{\textquoteright} initiating the dissociation of the coral-algal symbiosis during exposure to sub-optimal conditions. This study demonstrates how ex- 
posure to low-salinity seawater alone can cause some of the symptoms commonly attributed to temperature anomalies and anthropogenic pressures on coral reefs. },
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2003/Kerswell/Mar%20Ecol%20Prog%20Ser%202003%20Kerswell.pdf},
file = {{Mar Ecol Prog Ser 2003 Kerswell.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2003/Kerswell/Mar Ecol Prog Ser 2003 Kerswell.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/FB43F8EC-563B-4DBB-9613-78ADDDC353E6}}
}

