file = {{Mar Ecol Prog Ser 1997 Jones-2.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/1997/Jones/Mar Ecol Prog Ser 1997 Jones-2.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/25FAFD06-B1FD-4857-B255-0A7F01A15FB5}}
}

