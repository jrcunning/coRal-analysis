@article{Reynaud:2009fs,
author = {Reynaud, S and Martinez, P. and Houlbr{\`e}que, F and Billy, I. and Allemand, Denis and Ferrier-Pag{\`e}s, Christine},
title = {{Effect of light and feeding on the nitrogen isotopic composition of a zooxanthellate coral: role of nitrogen recycling}},
journal = {Marine Ecology Progress Series},
year = {2009},
volume = {392},
pages = {103--110},
affiliation = {Ctr Sci Monaco, MC-98000 Monaco, Monaco},
doi = {10.3354/meps08195},
language = {English},
rating = {0},
date-added = {2012-02-02T18:08:45GMT},
date-modified = {2016-09-10T19:14:05GMT},
abstract = {Experiments were performed in controlled conditions in order to investigate the effect of light and/or feeding on the delta N-15 composition of zooxanthellae and animal tissue of a scleractinian coral, Stylophora pistillata. For this purpose, corals were cultured under 3 light levels (80, 200, and 300 mu mol photons m(-2) s(-1)) and for each light level, half of the corals were fed twice a week with natural zooplankton while the other half, the control group, were starved. To understand how plankton ingestion changes the isotopic signal of the corals, zooplankton delta N-15 was also measured during the course of the experiment. It had a mean value of 6.75 parts per thousand. No significant and consistent effect of light was observed on the delta N-15 of the tissue or zooxanthellae of S. pistillata. However, there was a significant depletion in tissue and zooxanthellae N-15 of fed corals compared to the controls. This depletion did not follow the general food-web rule in which predators are enriched in N-15 by 3.5 parts per thousand compared to prey, but can be explained by the fact that nitrogen is recycled within the symbiotic association. Indeed, in heterotrophic organisms, the enrichment is due to light nitrogen (N-14) being preferentially excreted in the form of waste products into the environment, which leads to an increase in the delta N-15 signature of the animal. In corals, light waste products are instead recycled by the zooxanthellae, therefore depleting the isotopic signature of the zooxanthellae, and also of the host, by transfer of photosynthates with light nitrogen. These observations support the nitrogen-recycling rather than the nitrogen-conservation theory.},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2&SrcAuth=mekentosj&SrcApp=Papers&DestLinkType=FullRecord&DestApp=WOS&KeyUT=000271828700009},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2009/Reynaud/Mar%20Ecol%20Prog%20Ser%202009%20Reynaud.pdf},
file = {{Mar Ecol Prog Ser 2009 Reynaud.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2009/Reynaud/Mar Ecol Prog Ser 2009 Reynaud.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.3354/meps08195}}
}

