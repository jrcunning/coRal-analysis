file = {{Symbiosis 2000 Titlyanov.PDF:/Users/jrcunning/Documents/Library.papers3/Articles/2000/Titlyanov/Symbiosis 2000 Titlyanov.PDF:application/pdf}},
uri = {\url{papers3://publication/uuid/7B73E7A6-4344-4E31-80BF-CCEEA6CD340B}}
