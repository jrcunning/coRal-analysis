@article{Stone:1999p3812,
author = {Stone, L and Huppert, A and Rajagopalan, B and Bhasin, H and Loya, Yossi},
title = {{Mass coral reef bleaching: A recent outcome of increased El Nino activity?}},
journal = {Ecology Letters},
year = {1999},
volume = {2},
number = {5},
pages = {325--330},
affiliation = {Tel Aviv Univ, Porter Super Ctr Ecol {\&} Environm Studies, IL-69978 Ramat Aviv, Israel},
language = {English},
rating = {0},
date-added = {2009-02-04T18:18:20GMT},
date-modified = {2015-04-19T14:35:50GMT},
abstract = {Coral reefs are generally considered to be the most biologically productive of all marine ecosystems, but in recent times these vulnerable aquatic resources have been subject to unusual degradation. The general decline in reefs has been greatly accelerated by mass bleaching in which corals whiten en masse and often fail to recover. Empirical evidence indicates a coral reef bleaching cycle in which major bleaching episodes are synchronized with El Nino events that occur every 3-4 years on average. By heating vast areas of the Pacific Ocean, and affecting the Indian and Atlantic Oceans as well, El Nino causes widespread damage to reefs largely because corals are very sensitive to temperature changes. However, mass bleaching events were rarely observed before the 1970s and their abrupt appearance two decades ago remains an enigma. Here we propose a new explanation for the sudden occurrence of mass bleaching and show that it may be a response to the relative increase in El Nino experienced over the last two decades.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/1999/Stone/Ecol%20Lett%201999%20Stone.pdf},
file = {{Ecol Lett 1999 Stone.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/1999/Stone/Ecol Lett 1999 Stone.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/E02FD6E6-47AB-44F5-83E3-9813ED5A0B82}}
}

