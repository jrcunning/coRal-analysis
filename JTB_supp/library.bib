@article{Albright:2008p271,
author = {Albright, Rebecca and Mason, Benjamin and Langdon, Chris},
title = {{Effect of aragonite saturation state on settlement and post-settlement growth of Porites astreoides larvae}},
journal = {Coral Reefs},
year = {2008},
volume = {27},
number = {3},
pages = {485--490},
affiliation = {Univ Miami, Rosenstiel Sch Marine {\&} Atmospher Sci, Div Marine Biol {\&} Fisheries, Miami, FL 33149 USA},
doi = {10.1007/s00338-008-0392-5},
language = {English},
read = {Yes},
rating = {0},
date-added = {2008-10-13T14:45:33GMT},
date-modified = {2015-04-19T14:38:54GMT},
abstract = {In response to the increases in pCO(2) projected in the 21st century, adult coral growth and calcification are expected to decrease significantly. However, no published studies have investigated the effect of elevated pCO(2) on earlier life history stages of corals. Porites astreoides larvae were collected from reefs in Key Largo, Florida, USA, settled and reared in controlled saturation state seawater. Three saturation states were obtained, using 1 M HCl additions, corresponding to present (380 ppm) and projected pCO(2) scenarios for the years 2065 (560 ppm) and 2100 (720 ppm). The effect of saturation state on settlement and post-settlement growth was evaluated. Saturation state had no significant effect on percent settlement; however, skeletal extension rate was positively correlated with saturation state, with similar to 50% and 78% reductions in growth at the mid and high pCO(2) treatments compared to controls, respectively.},
url = {http://www.springerlink.com/content/k203241747566171/},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2008/Albright/Coral%20Reefs%202008%20Albright.pdf},
file = {{Coral Reefs 2008 Albright.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2008/Albright/Coral Reefs 2008 Albright.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1007/s00338-008-0392-5}}
}

