title = {{Temporal and spatial expression patterns of biomineralization proteins during early development in the stony coral Pocillopora damicornis}},
journal = {Proceedings of the Royal Society B},
year = {2016},
volume = {283},
number = {1829},
pages = {20160322},
month = apr,
