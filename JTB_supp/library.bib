@article{Bellantuono:2012cd,
author = {Bellantuono, Anthony J and Hoegh-Guldberg, Ove and Rodriguez-Lanetty, Mauricio},
title = {{Resistance to thermal stress in corals without changes in symbiont composition.}},
journal = {Proceedings of the Royal Society B},
year = {2012},
volume = {279},
number = {1731},
pages = {1100--1107},
month = mar,
affiliation = {Department of Biology, University of Louisiana at Lafayette, Lafayette, LA 70504, USA.},
doi = {10.1098/rspb.2011.1780},
pmid = {21976690},
pmcid = {PMC3267153},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-10-10T14:26:15GMT},
date-modified = {2016-09-10T19:14:06GMT},
abstract = {Discovering how corals can adjust their thermal sensitivity in the context of global climate change is important in understanding the long-term persistence of coral reefs. In this study, we showed that short-term preconditioning to higher temperatures, 3{\textdegree}C below the experimentally determined bleaching threshold, for a period of 10 days provides thermal tolerance for the symbiosis stability between the scleractinian coral, Acropora millepora and Symbiodinium. Based on genotypic analysis, our results indicate that the acclimatization of this coral species to thermal stress does not come down to simple changes in Symbiodinium and/or the bacterial communities that associate with reef-building corals. This suggests that the physiological plasticity of the host and/or symbiotic components appears to play an important role in responding to ocean warming. The further study of host and symbiont physiology, both of Symbiodinium and prokaryotes, is of paramount importance in the context of global climate change, as mechanisms for rapid holobiont acclimatization will become increasingly important to the long-standing persistence of coral reefs.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21976690&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2012/Bellantuono/Proc%20R%20Soc%20B%202012%20Bellantuono.pdf},
file = {{Proc R Soc B 2012 Bellantuono.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2012/Bellantuono/Proc R Soc B 2012 Bellantuono.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1098/rspb.2011.1780}}
}

