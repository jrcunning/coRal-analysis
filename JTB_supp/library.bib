file = {{Mar Ecol Prog Ser 2009 Stat.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2009/Stat/Mar Ecol Prog Ser 2009 Stat.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/B4576BC1-959A-4CD4-B3E3-8E550E8ACCEA}}
}

