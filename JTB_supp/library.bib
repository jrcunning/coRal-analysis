@article{Rodriguez:2009il,
author = {Rodriguez, R. J. and White, J. F. Jr and Arnold, A. E. and Redman, R. S.},
title = {{Fungal endophytes: diversity and functional roles}},
journal = {New Phytologist},
year = {2009},
volume = {182},
number = {2},
pages = {314--330},
affiliation = {US Geol Survey, Seattle, WA USA},
doi = {10.1111/j.1469-8137.2009.02773.x},
language = {English},
rating = {0},
date-added = {2012-02-10T21:21:22GMT},
date-modified = {2016-09-10T19:14:05GMT},
abstract = {All plants in natural ecosystems appear to be symbiotic with fungal endophytes. This highly diverse group of fungi can have profound impacts on plant communities through increasing fitness by conferring abiotic and biotic stress tolerance, increasing biomass and decreasing water consumption, or decreasing fitness by altering resource allocation. Despite more than 100 yr of research resulting in thousands of journal articles, the ecological significance of these fungi remains poorly characterized. Historically, two endophytic groups (clavicipitaceous (C) and nonclavicipitaceous (NC)) have been discriminated based on phylogeny and life history traits. Here, we show that NC-endophytes represent three distinct functional groups based on host colonization and transmission, in planta biodiversity and fitness benefits conferred to hosts. Using this framework, we contrast the life histories, interactions with hosts and potential roles in plant ecophysiology of C-and NC-endophytes, and highlight several key questions for future work in endophyte biology.},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2&SrcAuth=mekentosj&SrcApp=Papers&DestLinkType=FullRecord&DestApp=WOS&KeyUT=000264635400007},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2009/Rodriguez/New%20Phytol%202009%20Rodriguez.pdf},
file = {{New Phytol 2009 Rodriguez.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2009/Rodriguez/New Phytol 2009 Rodriguez.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1111/j.1469-8137.2009.02773.x}}
}

