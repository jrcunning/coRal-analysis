@article{Tolleter:2013ij,
author = {Tolleter, Dimitri and Seneca, Francois O and DeNofrio, Jan C and Krediet, Cory J and Palumbi, Stephen R and Pringle, John R and Grossman, Arthur R},
title = {{Coral bleaching independent of photosynthetic activity.}},
journal = {Current Biology},
year = {2013},
volume = {23},
number = {18},
pages = {1782--1786},
month = sep,
publisher = {Elsevier},
affiliation = {Department of Plant Biology, Carnegie Institution for Science, Stanford, CA 94305, USA.},
doi = {10.1016/j.cub.2013.07.041},
pmid = {24012312},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-09-06T20:37:31GMT},
date-modified = {2016-09-10T19:14:04GMT},
abstract = {The global decline of reef-building corals is due in part to the loss of algal symbionts, or "bleaching," during the increasingly frequent periods of high seawater temperatures. During bleaching, endosymbiotic dinoflagellate algae (Symbiodinium spp.) either are lost from the animal tissue or lose their photosynthetic pigments, resulting in host mortality if the Symbiodinium populations fail to recover. The >1,000 studies of the causes of heat-induced bleaching have focused overwhelmingly on the consequences of damage to algal photosynthetic processes, and the prevailing model for bleaching invokes a light-dependent generation of toxic reactive oxygen species (ROS) by heat-damaged chloroplasts as the primary trigger. However, the precise mechanisms of bleaching remain unknown, and there is evidence for involvement of multiple cellular processes. In this study, we asked the simple question of whether bleaching can be triggered by heat in the dark, in the absence of photosynthetically derived ROS. We used both the sea anemone model system Aiptasia and several species of reef-building corals to demonstrate that symbiont loss can occur rapidly during heat stress in complete darkness. Furthermore, we observed damage to the photosynthetic apparatus under these conditions in both Aiptasia endosymbionts and cultured Symbiodinium. These results do not directly contradict the view that light-stimulated ROS production is important in bleaching, but they do show that there must be another pathway leading to bleaching. Elucidation of this pathway should help to clarify bleaching mechanisms under the more usual conditions of heat stress in the light.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0960982213008646},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2013/Tolleter/Current%20Biology%202013%20Tolleter.pdf},
file = {{Current Biology 2013 Tolleter.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/Tolleter/Current Biology 2013 Tolleter.pdf:application/pdf;Current Biology 2013 Tolleter.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/Tolleter/Current Biology 2013 Tolleter.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.cub.2013.07.041}}
}

@article{Santos:2002p3868,
author = {Santos, Scott R and Taylor, DJ and Kinzie III, Robert A and Sakai, K and Coffroth, Mary Alice},
title = {{Evolution of length variation and heteroplasmy in the chloroplast rDNA of symbiotic dinoflagellates (Symbiodinium, Dinophyta) and a novel insertion in the universal core region of the large subunit rDNA}},
journal = {Phycologia},
year = {2002},
volume = {41},
number = {4},
pages = {311--318},
affiliation = {SUNY Buffalo, Dept Biol Sci, Buffalo, NY 14260 USA},
language = {English},
rating = {0},
date-added = {2009-02-04T18:19:03GMT},
date-modified = {2015-04-19T14:35:34GMT},
abstract = {Marine dinoflagellates are a diverse and ecologically important group of unicellular protists responsible for events such as 'red tides' and for forming mutualistic symbioses with various invertebrates. However, little is known about the evolution of organellar DNA in these organisms. We analysed domain V of the chloroplast large subunit ribosomal DNA (cp23S-rDNA) from a variety of symbiotic dinoflagellates and uncovered several unique features. These included length heteroplasmy of cp23S-rDNA that resulted from deletion mutations and the presence of a 1-13 bp nucleotide insertion in the universal core region. The insertion in the large subunit rDNA is novel across all kingdoms and appears to be a unique derived state for the symbiotic genus Symbiodinium. In addition, analysis of expansion segments (ES) revealed that the cp23S-rDNA domain V of symbiotic dinoflagellates is the most size variable of any known plastid-harbouring organism. Our results demonstrate that the universal core secondary structure and the chloroplast rDNA ES are less constrained in size than previously thought.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2002/Santos/Phycologia%202002%20Santos.pdf},
file = {{Phycologia 2002 Santos.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2002/Santos/Phycologia 2002 Santos.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/52553961-75AF-43BC-9AA8-3B7DF5CA2D05}}
}

@article{VanOppen:2001p7935,
author = {van Oppen, Madeleine J H and McDonald, BJ and Willis, Bette L and Miller, David J},
title = {{The evolutionary history of the coral genus Acropora (Scleractinia, Cnidaria) based on a mitochondrial and a nuclear marker: Reticulation, incomplete lineage sorting, or morphological convergence?}},
journal = {Molecular Biology And Evolution},
year = {2001},
volume = {18},
number = {7},
pages = {1315--1329},
affiliation = {James Cook Univ N Queensland, Townsville, Qld 4811, Australia},
language = {English},
read = {Yes},
rating = {0},
date-added = {2010-08-16T17:23:22GMT},
date-modified = {2015-11-10T23:50:24GMT},
abstract = {This study examines molecular relationships across a wide range of species in the mass spawning scleractinian coral genus Acropora. Molecular phylogenies were obtained for 28 species using DNA sequence analyses of two independent markers, a nuclear intron and the mtDNA putative control region. Although the compositions of the major clades in the phylogenies based on these two markers were similar, there were several important differences. This, in combination with the fact that many species were not monophyletic, suggests either that introgressive hybridization is occurring or that lineage sorting is incomplete. The molecular tree topologies bear little similarity to the results of a recent cladistic analysis based on skeletal morphology and are at odds with the fossil record. We hypothesize that these conflicting results may be due to the same morphology having evolved independently more than once in Acropora and/or the occurrence of extensive interspecific hybridization and introgression in combination with morphology being determined by a small number of genes. Our results indicate that many Acropora species belong to a species complex or syngameon and that morphology has little predictive value with regard to syngameon composition. Morphological species in the genus often do not correspond to genetically distinct evolutionary units. Instead, species that differ in timing of gamete release tend to constitute genetically distinct clades.},
url = {http://apps.isiknowledge.com/InboundService.do?Func=Frame&product=WOS&action=retrieve&SrcApp=Papers&UT=000169846400015&SID=1DljghJ%402AIgkkP%40o34&Init=Yes&SrcAuth=mekentosj&mode=FullRecord&customersID=mekentosj&DestFail=http%3A%2F%2Faccess.isiproducts.com%2Fcustom_images%2Fwok_failed_auth.html},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2001/van%20Oppen/Mol%20Biol%20Evol%202001%20van%20Oppen.pdf},
file = {{Mol Biol Evol 2001 van Oppen.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2001/van Oppen/Mol Biol Evol 2001 van Oppen.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/5730B14C-6A45-4A62-BCF7-EC04A17AC57F}}
}

