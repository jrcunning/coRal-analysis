pages = {149--155},
affiliation = {Soka Univ, Tokyo 1928577, Japan},
language = {English},
rating = {0},
date-added = {2009-02-04T18:18:08GMT},
date-modified = {2015-04-19T14:33:50GMT},
abstract = {Physiological characteristics of zooxanthellae were examined under nutrient-saturated conditions created by mixing ammonium ((NH4)-N-15) with nitrate ((NO3)-N-15) to give 0.88 mM total nitrogen. Growth rate varied with the form Of nitrogen provided. Ammonium alone resulted in the lowest C:N and C:chl-tr ratios. Although zooxanthellae took up nitrate in the absence of ammonium, ammonium assimilation was 1.3 times higher than nitrate assimilation. Ammonium strongly inhibited nitrate assimilation. While high-ammonium treatments resulted in the highest C-14 incorporation into intermediate compounds, high nitrate levels resulted in the highest C-14 incorporation into protein, suggesting that the intermediate compounds are produced prior to the subsequent production of protein when ammonium is the dominant N source. The enhanced production of intermediate compounds at the expense of carbon directed to protein synthesis in the presence of ammonium might be analogous to the "host factor" observed in zooxanthellae-host symbioses, since growth rate is depressed due to low production of protein.},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2001/Taguchi/Mar%20Biol%202001%20Taguchi.pdf},
file = {{Mar Biol 2001 Taguchi.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2001/Taguchi/Mar Biol 2001 Taguchi.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/83C2C2F7-6AD4-4F1A-A053-77509882D13A}}
}

