@article{Riegl:2011go,
author = {Riegl, Bernhard M and Purkis, Sam J and Al-Cibahy, Ashraf S and Abdel-Moati, Mohammed A and Hoegh-Guldberg, Ove},
title = {{Present limits to heat-adaptability in corals and population-level responses to climate extremes.}},
journal = {PLoS ONE},
year = {2011},
volume = {6},
number = {9},
pages = {e24802},
affiliation = {National Coral Reef Institute, Nova Southeastern University, Dania, Florida, United States of America. rieglb@nova.edu},
doi = {10.1371/journal.pone.0024802},
pmid = {21949755},
pmcid = {PMC3176771},
language = {English},
rating = {0},
date-added = {2012-12-26T21:58:13GMT},
date-modified = {2016-09-10T19:14:01GMT},
abstract = {Climate change scenarios suggest an increase in tropical ocean temperature by 1-3{\textdegree}C by 2099, potentially killing many coral reefs. But Arabian/Persian Gulf corals already exist in this future thermal environment predicted for most tropical reefs and survived severe bleaching in 2010, one of the hottest years on record. Exposure to 33-35{\textdegree}C was on average twice as long as in non-bleaching years. Gulf corals bleached after exposure to temperatures above 34{\textdegree}C for a total of 8 weeks of which 3 weeks were above 35{\textdegree}C. This is more heat than any other corals can survive, providing an insight into the present limits of holobiont adaptation. We show that average temperatures as well as heat-waves in the Gulf have been increasing, that coral population levels will fluctuate strongly, and reef-building capability will be compromised. This, in combination with ocean acidification and significant local threats posed by rampant coastal development puts even these most heat-adapted corals at risk. WWF considers the Gulf ecoregion as "critically endangered". We argue here that Gulf corals should be considered for assisted migration to the tropical Indo-Pacific. This would have the double benefit of avoiding local extinction of the world's most heat-adapted holobionts while at the same time introducing their genetic information to populations na{\"\i}ve to such extremes, potentially assisting their survival. Thus, the heat-adaptation acquired by Gulf corals over 6 k, could benefit tropical Indo-Pacific corals who have <100 y until they will experience a similarly harsh climate. Population models suggest that the heat-adapted corals could become dominant on tropical reefs within ∼20 years.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21949755&retmode=ref&cmd=prlinks},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0024802}}
}

