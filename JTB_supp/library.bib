@article{MacGregorFors:2013jc,
author = {MacGregor-Fors, Ian and Payton, Mark E},
title = {{Contrasting Diversity Values: Statistical Inferences Based on Overlapping Confidence Intervals}},
journal = {PLoS ONE},
year = {2013},
volume = {8},
number = {2},
pages = {e56794},
month = feb,
publisher = {Public Library of Science},
doi = {10.1371/journal.pone.0056794},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-10-05T21:50:20GMT},
date-modified = {2016-09-10T19:14:18GMT},
abstract = {Ecologists often contrast diversity (species richness and abundances) using tests for comparing means or indices. However, many popular software applications do not support performing standard inferential statistics for estimates of species richness and/or density. In this study we simulated the behavior of asymmetric log-normal confidence intervals and determined an interval level that mimics statistical tests with P($\alpha$) = 0.05 when confidence intervals from two distributions do not overlap. Our results show that 84% confidence intervals robustly mimic 0.05 statistical tests for asymmetric confidence intervals, as has been demonstrated for symmetric ones in the past. Finally, we provide detailed user-guides for calculating 84% confidence intervals in two of the most robust and highly-used freeware related to diversity measurements for wildlife (i.e., EstimateS , Distance ).},
url = {http://dx.plos.org/10.1371/journal.pone.0056794},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2013/MacGregor-Fors/PLoS%20ONE%202013%20MacGregor-Fors.pdf},
file = {{PLoS ONE 2013 MacGregor-Fors.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/MacGregor-Fors/PLoS ONE 2013 MacGregor-Fors.pdf:application/pdf;PLoS ONE 2013 MacGregor-Fors.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2013/MacGregor-Fors/PLoS ONE 2013 MacGregor-Fors.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0056794}}
}

