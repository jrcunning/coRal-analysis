file = {{Mar Ecol Prog Ser 2003 Jones.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2003/Jones/Mar Ecol Prog Ser 2003 Jones.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/1B526C0B-A5CB-433A-984C-D570F35CC763}}
}

