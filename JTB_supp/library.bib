file = {{PLoS ONE 2011 Kvitt.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2011/Kvitt/PLoS ONE 2011 Kvitt.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0028665}}
}

