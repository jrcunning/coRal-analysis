@article{Moriya:2007cg,
author = {Moriya, Yuki and Itoh, Masumi and Okuda, Shujiro and Yoshizawa, Akiyasu C and Kanehisa, Minoru},
title = {{KAAS: an automatic genome annotation and pathway reconstruction server.}},
journal = {Nucleic acids research},
year = {2007},
volume = {35},
number = {Web Server issue},
pages = {W182--5},
month = jul,
affiliation = {Bioinformatics Center, Institute for Chemical Research, Kyoto University, Gokasho, Uji, Kyoto 611-0011, Japan.},
