@article{Kooijman:2008ew,
author = {Kooijman, Sebastiaan A L M and Sousa, T and Pecquerie, L and van der Meer, J and Jager, T},
title = {{From food-dependent statistics to metabolic parameters, a practical guide to the use of dynamic energy budget theory.}},
journal = {Biological Reviews},
year = {2008},
volume = {83},
number = {4},
pages = {533--552},
month = nov,
affiliation = {Department of Theoretical Biology, Vrije Universiteit, de Boelelaan 1087, 1081 HV Amsterdam, The Netherlands. bas@bio.vu.nl},
doi = {10.1111/j.1469-185X.2008.00053.x},
pmid = {19016672},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-09-27T18:58:34GMT},
date-modified = {2016-09-10T19:14:08GMT},
abstract = {The standard model of the dynamic energy budget theory for metabolic organisation has variables and parameters that can be quantified using indirect methods only. We present new methods (and software) to extract food-independent parameter values of the energy budget from food-dependent quantities that are easy to observe, and so facilitate the practical application of the theory to enhance predictability and extrapolation. A natural sequence of 10 steps is discussed to obtain some compound parameters first, then the primary parameters, then the composition parameters and finally the thermodynamic parameters; this sequence matches a sequence of required data of increasing complexity which is discussed in detail. Many applications do not require knowledge of all parameters, and we discuss methods to extrapolate parameters from one species to another. The conversion of mass, volume and energy measures of biomass is discussed; these conversions are not trivial because biomass can change in chemical composition in particular ways thanks to different forms of homeostasis. We solve problems like "What would be the ultimate reproduction rate and the von Bertalanffy growth rate at a specific food level, given that we have measured these statistics at abundant food?" and "What would be the maximum incubation time, given the parameters of the von Bertalanffy growth curve?". We propose a new non-destructive method for quantifying the chemical potential and entropy of living reserve and structure, that can potentially change our ideas on the thermodynamic properties of life. We illustrate the methods using data on daphnids and molluscs.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19016672&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jrcunning/Documents/Library.papers3/Articles/2008/Kooijman/Biol%20Rev%202008%20Kooijman.pdf},
file = {{Biol Rev 2008 Kooijman.pdf:/Users/jrcunning/Documents/Library.papers3/Articles/2008/Kooijman/Biol Rev 2008 Kooijman.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1111/j.1469-185X.2008.00053.x}}
}

