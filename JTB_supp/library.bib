@article{Scully:2001p3946,
author = {Scully, EP and Prappas, J and Ostrander, Gary K},
title = {{Laboratory models for the study of coral pathologies}},
journal = {Hydrobiologia},
year = {2001},
volume = {460},
pages = {91--95},
